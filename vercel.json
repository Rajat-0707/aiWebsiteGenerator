{
  "version": 2,
  "builds": [
    {
      "src": "server/vercel.js",
      "use": "@vercel/node"
    },
    {
      "src": "aiwebsitemaker/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/vercel.js"
    },
    {
      "src": "/(.*)",
      "dest": "aiwebsitemaker/build/$1"
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/" }] }


